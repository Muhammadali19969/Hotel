<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryBackground" Color="#172649"/>
    <SolidColorBrush x:Key="SecondaryBackground" Color="#DEDEDE"/>
    <SolidColorBrush x:Key="SelectedBackground" Color="#50656E"/>

    <SolidColorBrush x:Key="PrimaryBorderColor" Color="#DEDEDE"/>
    <SolidColorBrush x:Key="SecondaryBorderColor" Color="#17B7E8"/>
    <SolidColorBrush x:Key="PrimaryTextColor" Color="#E3E3E3"/>

</ResourceDictionary>